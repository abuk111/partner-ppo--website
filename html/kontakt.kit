<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Kontakt | PARTNER PPOŻ</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
        href="https://fonts.googleapis.com/css2?family=Plus+Jakarta+Sans:wght@300;500&family=Raleway:ital,wght@0,200;0,400;0,700;1,200;1,400&display=swap"
        rel="stylesheet">
    <script src="https://kit.fontawesome.com/a8324f3062.js" crossorigin="anonymous"></script>

    <script src="https://www.google.com/recaptcha/api.js" async defer></script>

    <link rel="stylesheet" href="./dist/css/main-min.css">
    <link rel="stylesheet" href="./dist/css/contact-min.css">
</head>

<body>

    <!-- @include nav.kit -->

    <!-- @include header-subpages.kit -->


    <section class="contact medium-bgc section">
        <div class="wrapper">
            <h1 class="contact__head section-head">Kontakt</h1>
            <div class="contact__gridbox">
                <div class="contact__contactbox">
                    <div class="contact__contactbox__adressbox">
                        <h2 class="contact__contactbox__adressbox-head">Dane firmy</h2>
                        <h3 class="contact__contactbox__adressbox-name">
                            <strong>Usługi przeciwpożarowe</strong>
                        </h3>
                        <p class="contact__contaxtbox__adressbox-name-p ">
                            <strong>PARTNER PPOŻ</strong>
                        </p>
                        <p class="contact__contaxtbox__adressbox-name-p ">
                            Piotr Kulik
                        </p>

                        <div class="contact__contactbox__adressbox-adress">
                            <i class="fas fa-map-marker-alt"></i>
                            <p>42-580 Wojkowice <br> ul. Piaski 12a</p>
                        </div>

                        <p class="contact__contactbox__adressbox-nip">
                            <i class="far fa-id-card"></i> NIP: 625-117-50-94
                        </p>
                    </div>

                    <div class="contact__contactbox__mailbox">
                        <h2 class="contact__contactbox__mailbox-head footer-header">Dane kontaktowe</h2>
                        <div class="contact__contactbox__mailbox-email">
                            <i class="far fa-envelope"></i>
                            <p> <a href="mailto:partnerppoz@wp.pl">partnerppoz@wp.pl</a> <br> <a
                                    href="mailto:info@partnerppoz.pl">info@partnerppoz.pl</a></p>
                        </div>
                        <div class="contact__contactbox__mailbox-phone"><i class="fas fa-phone"></i>
                            <p><a href="tel:0048517204340">+48 517 204
                                    340</a></p>
                        </div>
                    </div>

                </div>

                <div class="contact__mapbox">
                    <h2 class="contact__mapbox-head">Nasza lokalizacja</h2>
                    <iframe
                        src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d636.2052121744911!2d19.017731929264468!3d50.369894954111686!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x4716d58d7d5a289b%3A0x386632f7af40f5b5!2sPartnerPpo%C5%BC%20Piotr%20Kulik!5e0!3m2!1spl!2spl!4v1679758851870!5m2!1spl!2spl"
                        width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy"
                        referrerpolicy="no-referrer-when-downgrade"></iframe>
                </div>
            </div>
        </div>
    </section>

    <section class="form medium-bgc section" id="contact-form" action="?" method="POST">
        <div class="wrapper">
            <h2 class="form__header">Napisz do nas!</h2>
            <form class="form__formbox">
                <div class="form__formbox__formelement">
                    <label for="name">Imię i nazwisko:</label>
                    <input type="text" id="name" placeholder="Podaj swoje imię i nazwisko">
                    <p class="error-text">error</p>
                </div>
                <div class="form__formbox__formelement">
                    <label for="mail">Adres e-mail</label>
                    <input type="text" id="mail" placeholder="Podaj swój e-mail">
                    <p class="error-text">error</p>
                </div>
                <div class="form__formbox__formelement">
                    <label for="phone">Telefon:</label>
                    <input type="text" id="phone" placeholder="Podaj swój numer telefonu">
                    <p class="error-text">error</p>
                </div>
                <div class="form__formbox__formelement">
                    <label for="message">Wiadomość:</label>
                    <textarea type="text" id="message" placeholder="Wpisz wiadomość"></textarea>
                    <p class="error-text">error</p>
                </div>
                <div class="g-recaptcha" data-sitekey="6LdNTE0lAAAAAOkA1JT44dxW70YmqmOOjRTafaJH" data-size="invisible"
                    data-callback="verifyCaptcha">
                </div>
                <button type="submit" class="form__formbox__send" id="submit">Wyślij!</button>
            </form>
        </div>
    </section>

    <!-- @include footer.kit -->


    <script src="./dist/js/script-min.js"></script>

</body>

</html>